{
    "name": "example/backend",
    "description": "Yii2 backend project",
    "type": "project",
    "require": {
        "php": ">=8.0.0",
        "firebase/php-jwt": "^6.9",
        "yidas/yii2-bower-asset": "^2.0",
        "yiisoft/yii2": "~2.0.47"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "process-timeout": 1800,
        "sort-packages": true,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "app\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}
